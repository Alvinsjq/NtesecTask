MODULE main

IVAR
	B4_0 : -10..10;
	B5_0 : -10..10;
	B6_0 : -10..10;
	B7_0 : -10..10;
	B12_0 : -10..10;

VAR
	B10_0 : boolean;

DEFINE
	B1_0 := (1*B4_0 + 0) * (2*B5_0 + 1);
	B2_0 := (1*B6_0 + 1) * (2*B7_0 + 0);
	B3_0 := 2*B1_0 + 1*B2_0 + 1;
	B8_0 := 
		case
			B3_0 >= -10 & B3_0 <= 10 : B3_0;
			B3_0 < -10 : -10;
			B3_0 > 10 : 10;
		esac;
	B9_0 := B8_0 = B12_0 ? TRUE : FALSE;

ASSIGN
	init(B10_0) := FALSE;
	next(B10_0) := B9_0;

